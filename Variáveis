class Variaveis {
    public static void main (String [] args){
       String nome = "Nagela";
       int idade = 20;
       String nomeDoMeuCachorro = "Zeus";

       System.out.println ("Nome: " + nome);
       System.out.println ("Idade: " + idade);
       System.out.println ("Nome do meu Cachorro: " + nomeDoMeuCachorro);
    } 
}
