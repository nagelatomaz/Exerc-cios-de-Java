class TipoPrimitivo {
    public static void main (String [] args){
        byte idade01 = 20;
        short idade02 = 21;
        int idade03 = 22;
        long idade04 = 23;

    System.out.println ("Valor da idade 01" + idade01);
    System.out.println ("Valor da idade 02" + idade02);
    System.out.println ("Valor da idade 03" + idade03);
    System.out.println ("Valor da idade 04" + idade04);

    }
}
